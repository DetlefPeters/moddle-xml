{
  "name": "moddle-xml",
  "version": "1.1.1",
  "description": "XML import/export for documents described with moddle",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "all": "run-s lint test",
    "lint": "eslint {lib,test}/**/*.js",
