    "url": "https://github.com/Nikku"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^3.19.0",
    "eslint-plugin-mocha": "^4.11.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1"
  },
  "dependencies": {
    "lodash": "^3.0.0",
    "moddle": "^1.0.0",
    "saxen": "^5.0.0",
    "tiny-stack": "^0.1.0"
